{
      "HelloWorldMessage": {
         "description": "hello world Message",
         "type": "object",
         "required": [
           "message"
         ],
         "properties": {
           "message": {
              "type": "string"
           }
         }
      },
      "PlainMessage": {
         "description": "Plain Object Message",
         "type": "object",
         "required": [
            "message"
         ],
         "properties": {
            "message": {
               "type": "string",
               "example": "The protected test route is functional!"
            }
         }
      },
      "ErrorResponse": {
         "title": "MultiError",
         "description": "Gives full information about the problem",
         "type": "object",
         "required": [
            "code", "message"
         ],
         "properties": {
            "code": {
              "description": "A human readable code (death to numeric error codes!)",
               "type": "string",
               "enum": [
                  "DBERR", "NTERR", "UNERR"
               ],
               "example": "UNERR"
            },
            "message": {
               "ref": "#/definitions/ErrorMessage"
            }
         }
      },
      "ErrorMessage": {
         "title": "MultiErrorMessage",
         "description": "An error message with a long and a short description",
         "type": "object",
         "required": [
            "longMessage", "shortMessage"
         ],
         "properties": {
           "longMessage": {
              "description": "A long error description",
              "type": "string"
           },
            "shortMessage": {
               "description": "A short error description",
               "type": "string"
            }
         }
      }
   }

<!-- Header
           ============================================= -->
<header id="header" class="sticky-header">

  <div id="header-wrap">

    <div class="container clearfix">

      <div id="primary-menu-trigger"><i class="icon-reorder"></i></div>

      <!-- Logo
              ============================================= -->
      <div id="logo" ronterLink="/">
        <a href="/" class="standard-logo">
          <img src="/images/logo_cozyhouzz.png" alt="cozy Logo">
        </a>
        <a href="/" class="retina-logo">
          <img src="/images/logo_cozyhouzz@2x.png" alt="cozyhouzz Logo">
        </a>
      </div>

      <!-- Primary Navigation
              ============================================= -->
      <nav id="primary-menu" class="mobile-menu-off-canvas">

        <ul>
          <li>
            <a href="/">
              <div><strong>리스트로 보기</strong></div>
            </a>
          </li>
          <li>
            <a href="/post/room">
              <div><strong>지도로 보기</strong></div>
            </a>
            <ul>
              <li>
                <a href="/post/room">
                  <div><i class="icon-stack"></i>방 정보 목록조회</div>
                </a>
              </li>
            </ul>
          </li>
          <li>
            <a href="/post/new">
              <div><strong>NEW</strong></div>
            </a>
          </li>
          <li>
            <a href="/consultingListInfo">
              <div><strong>채팅</strong></div>
            </a>
            <ul>
              <li>
                <a href="/post/new">
                  <div><i class="icon-stack"></i>질문하기</div>
                </a>
              </li>
            </ul>
          </li>
        </ul>

        <!-- Top Cart
						============================================= -->
        <div id="top-cart">
          <% if (logined) { %>
            <a href="/user/<%= userIdx %>">
              <i class="icon-user"></i><!--<span>5</span>-->
            </a>
          <% } else { %>
            <a href="/auth/login">
              <i class="icon-user"></i>
            </a>
          <% } %>
            <!--<div class="top-cart-content">-->

            <div class="top-cart-items">


            </div>
              <!--</div>-->
        </div>
        <!-- #top-cart end -->

        <!-- Top Search
						============================================= -->
        <div id="top-search">
          <a href="#" id="top-search-trigger"><i class="icon-search3"></i><i class="icon-line-cross"></i></a>
          <form>
            <input type="text" name="q" class="form-control typeahead" value="" placeholder="Type &amp; Hit Enter..">
          </form>
        </div>
        <!-- #top-search end -->

      </nav>
      <!-- #primary-menu end -->

    </div>

  </div>

</header>
<!-- #header end -->


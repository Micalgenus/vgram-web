<head>
   <!--<title>구글지도</title>-->


   <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&language=ko"></script>

   <script>
      function initialize() {
         var myLatlng = new google.maps.LatLng(35.840716, 127.131531); // y, x좌표값
         var mapOptions = {
            zoom: 15,
            center: myLatlng,
            mapTypeId: google.maps.MapTypeId.ROADMAP
         }


         var map = new google.maps.Map(document.getElementById('map_canvas'), mapOptions);
         var marker = new google.maps.Marker({
            position: myLatlng,
            map: map,
            title: "모빌리티랩스"
         });
         var infowindow = new google.maps.InfoWindow(
            {
               content: "<h4>모빌리티랩스</h4>",
               maxWidth: 300
            }
         );

         google.maps.event.addListener(marker, 'click', function() {
            infowindow.open(map, marker);
         });
      }
   </script>

</head>

<body onload="initialize()">

<!--바바 height:469px;-->
<div id="map_canvas" style="width:600px; height:480px;"></div>

</body>


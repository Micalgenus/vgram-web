<% for (var i = 0; i < data.length; i++) { %>
   <article class="portfolio-item" data-id="<%= data[i].ID %>" style="width: 33%;">
    <div class="portfolio-image">
      <div class="fslider" data-arrows="false">
        <div class="flexslider">
          <div class="slider-wrap">
             <div class="slide"><a href="portfolio-single-gallery.html"><img src="<%= mediaUrl %>/medias/vtours/sinho0689@gmail.com/201705310101/panos/SAM_100_0073.tiles/thumb.jpg" alt="Shake It"></a></div>
             <div class="slide"><a href="portfolio-single-gallery.html"><img src="<%= mediaUrl %>/medias/vtours/sinho0689@gmail.com/201705310101/panos/SAM_100_0074.tiles/thumb.jpg" alt="Shake It"></a></div>
             <div class="slide"><a href="portfolio-single-gallery.html"><img src="<%= mediaUrl %>/medias/vtours/sinho0689@gmail.com/201705310101/panos/SAM_100_0075.tiles/thumb.jpg" alt="Shake It"></a></div>
          </div>
        </div>
      </div>
      <div class="portfolio-overlay" data-lightbox="gallery">
         <a href="<%= mediaUrl %>/medias/vtours/sinho0689@gmail.com/201705310101/panos/SAM_100_0073.tiles/pano_f.jpg" class="left-icon" data-lightbox="gallery-item"><i class="icon-line-stack-2"></i></a>
         <a href="<%= mediaUrl %>/medias/vtours/sinho0689@gmail.com/201705310101/panos/SAM_100_0074.tiles/pano_f.jpg" class="hidden" data-lightbox="gallery-item"></a>
         <a href="<%= mediaUrl %>/medias/vtours/sinho0689@gmail.com/201705310101/panos/SAM_100_0075.tiles/pano_f.jpg" class="hidden" data-lightbox="gallery-item"></a>
        <!-- <a href="/lib/js-canvas/images/portfolio/full/1.jpg" class="right-icon"><i class="icon-line-ellipsis"></i></a> -->
      </div>
    </div>
    <div class="testimonial">
      <div class="testi-image">
        <a href="#"><img src="<%= mediaUrl %>/<%= data[i].user.profile_image_path %>" alt="Avatar"></a>
      </div>
      <div class="testi-meat">
        <p style=" margin-bottom:0px;"><%= data[i].user.nickname %></p>
      </div>
      <div class="clear"></div>
      <div class="portfolio-desc">
        <h3><a href="/post/<%= data[i].ID %>"><%= data[i].title %></a></h3>
      </div>
      <ul class="entry-meta clearfix">
        <li><i class="icon-line-heart"></i><span><%= data[i].LikeUsers.length %></span></li>
        <li><i class="icon-calendar3"></i><%= new Date(data[i].createdAt).toLocaleDateString("ko-KR", {year: 'numeric', month: '2-digit', day: '2-digit'}) %></li>
        <li><i class="icon-comments"></i><%= data[i].Comments.length %></li>
      </ul>
    </div>
  </article>

<% } %>

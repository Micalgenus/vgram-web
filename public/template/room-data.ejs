<div class="product col-md-6">
  <div class="product_area">
    <img src="<%= image_path %>">
    <div class="title_area">
      <h3><a href="#"><%= title %></a></h3>
    </div>
    <div class="address">
      <h4>주소</h4>
    </div>
    <div id="post">
      <ul class="entry-meta clearfix">
        <li><i class="icon-screenshot"></i>방종류 <%= room_type %></li>
        <li><i class="icon-screenshot"></i>보증금 <%= deposit %></li>
        <li><i class="icon-screenshot"></i>월세 <%= monthly_rent_fee %></li>
      </ul>
    </div>
  </div>
</div>
